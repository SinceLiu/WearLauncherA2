<com.readboy.wearlauncher.view.WatchDialTypeJ xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/watch_type_j_black_bg">

    <!--time-->
    <include
        layout="@layout/digit_clock_j_layout"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginLeft="22px"
        android:layout_marginTop="18px" />

    <TextView
        android:id="@+id/date_tvid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24px"
        android:layout_marginTop="66px"
        android:gravity="center"
        android:textColor="#ffffff"
        android:textSize="26sp"
        app:dateTemplate="MM/dd"
        />
</com.readboy.wearlauncher.view.WatchDialTypeJ>
