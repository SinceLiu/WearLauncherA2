<com.readboy.wearlauncher.view.WatchDialTypeL xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/watch_type_l_black_bg">

    <!--时钟-->
    <com.readboy.wearlauncher.view.AnalogClock
        android:id="@+id/analog_clock"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:analogHour="@drawable/watch_type_l_analog_hour"
        app:analogMinute="@drawable/watch_type_l_analog_min"
        app:analogSecond="@drawable/watch_type_l_analog_second"
        app:noSecond="false"/>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/watch_type_c_analog_dot"/>
</com.readboy.wearlauncher.view.WatchDialTypeL>
