<?xml version="1.0" encoding="utf-8"?>
<com.readboy.wearlauncher.view.DigitClock xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:id="@+id/digit_clock"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal" 
    android:gravity="center_horizontal"
	app:clockType="@integer/dial_type_a"
	app:dot="false"
	>
    
    <ImageView 
        android:id="@+id/hour0"
        android:layout_width="@dimen/num_clock_a_width"
    	android:layout_height="@dimen/num_clock_a_hight"
        />
    <ImageView 
        android:id="@+id/hour1"
		android:layout_width="@dimen/num_clock_a_width"
		android:layout_height="@dimen/num_clock_a_hight"
        />
    <ImageView 
        android:id="@+id/dot"
		android:layout_width="@dimen/num_clock_a_dot_width"
		android:layout_height="@dimen/num_clock_a_dot_hight"
        />
    <ImageView 
        android:id="@+id/min0"
		android:layout_width="@dimen/num_clock_a_width"
		android:layout_height="@dimen/num_clock_a_hight"
        />
    <ImageView 
        android:id="@+id/min1"
		android:layout_width="@dimen/num_clock_a_width"
		android:layout_height="@dimen/num_clock_a_hight"
        />
</com.readboy.wearlauncher.view.DigitClock>
